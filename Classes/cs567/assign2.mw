<Section collapsed="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title>
<Text-field style="Heading 2" layout="Heading 2">The 'div' routine - two interger division</Text-field></Title>
<Group labelreference="L735" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">idiv := proc(u1,v1,b,r::name)
  local u, v, Lu1, Lv1, dA, vn, vnt, un, d, n, m, i, j, k, qh, qhA, q, r1;

  Lu1 := ArrayNumElems(u1);
  Lv1 := ArrayNumElems(v1);
  u := Array(0..Lu1-1);
  v := Array(0..Lv1-1);
  Copy(u1,u);
  Copy(v1,v);

  if add(u[i],i=0..ArrayNumElems(u)-1) = 0 then
    r1 := 0;
    q := 0;
    r := r1; 
    return q;
  elif add(v[i],i=0..ArrayNumElems(v)-1) = 0 then
    error &quot;Divisor is 0!&quot;;;
  else
    n := ArrayNumElems(v);
    m := ArrayNumElems(u) - n;
    q := Array( 0..m+1-1 );
    d := floor( b / (v[0]+1) );
    dA := Array(0..0,[d]);
    vn := imult(v,dA,b);
    vnt := Array(0..n);
    vnt[0] := 0;
    for i from 1 to n do
      vnt[i] := vn[i-1];
    end do;

    #return(vnt);

    un := Array(0..m+n);
    un[0] := 0;
    for i from 1 to m+n do
      un[i] := u[i-1];
    end do;

    #return(un);

    if d &lt;&gt; 1 then
      un := imult(un,dA,b);
    end if;
    
    #return(un);

    #DEBUG();

    for j from 0 to m do
      if un[j] = vn[0] then
        qh := b - 1;
      else
        qh := floor( (un[j] * b + un[j+1]) / vn[0] );
      end if;
      if n &lt;&gt; 1 then
        for i while (vn[1]*qh) &gt; ((un[j]*b+un[j+1]-qh*vn[0])*b+un[j+2]) do
          qh := qh - 1;
        end do;
      end if;
      qhA := Array(0..0,[qh]);
      
      un[j..j+n] := isub( un[j..j+n], imult(vn,qhA,b), b, 'Borr' );
      q[j] := qh;
      if Borr &lt;&gt; 0 then
        q[j] := q[j] - 1;        
        un[j..j+n] := iadd( vnt, un[j..j+n], b, 'Carr' );
      end if;
    end do;
    r1 := idiv( un[m+1..m+n], dA, b, 'ignore' );
    #r1 := 0;
    r := r1;
  end if;
  return(q);
end;</Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display=""></Equation></Text-field>
</Output>
