<Group labelreference="L317" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">NewtonCal := proc( f, x0, THRESHOLD )
  local x, xn, cnt, err, iteration, N, i, temp;
  xn := x0;
  
  iteration := x - f(x) / diff( f(x), x );
  N := unapply( iteration, x );
  err := 1;
  cnt := 0;
  while err &gt; THRESHOLD do
    temp := xn;
    xn := N(xn);
    err := abs(xn - temp);
    cnt := cnt + 1;
  end do;
  print('`# of iterations`'=cnt);
  return xn;  
end;  </Text-field>
</Input>
