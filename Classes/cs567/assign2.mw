<Section collapsed="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title>
<Text-field style="Heading 2" layout="Heading 2">The 'sub' routine - two integers subtraction</Text-field></Title>
<Group labelreference="L751" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">isub := proc( A1, B1, b, Borr::name )
  local A, B, Ac, Bc, m, n, S, Bo, i;
  m := ArrayNumElems(A1);
  n := ArrayNumElems(B1);

  A := Array(0..m-1);
  B := Array(0..n-1);

  Copy(A1,A);
  Copy(B1,B);

  if m &gt; n then    
    Bc := Array(0..m-1);
    for i from 0 to m-1 do
      if i &lt;= m-n-1 then
        Bc[i] := 0;
      else
        Bc[i] := B[i-(m-n)]
      end if;
    end do;
    #print(convert(Bc, list));
    Ac := A;
  end if;
  if m &lt; n then
    Ac := Array(0..n-1);
    for i from 0 to n-1 do
      if i &lt;= n-m-1 then
        Ac[i] := 0;
      else
        Ac[i] := A[i-(n-m)]
      end if;
    end do;
    #print(convert(Ac, list));
    Bc := B;
  end if;
  if m = n then
    Ac := A;
    Bc := B;
  end if;
  S := Array(0..max(m,n)-1,'fill'=0);
  #Fill(max(m,n),0,S);
  Bo := 0;  
  for i from max(m,n)-1 by -1 to 0 do
    #print(i);
    if Ac[i] - Bc[i] - Bo &gt;= 0 then
      S[i] := Ac[i] - Bc[i] - Bo;
      Bo := 0;
    else
      S[i] := Ac[i] - Bc[i] - Bo + b;
      Bo := 1;
    end if;
  end do;
  Borr := Bo;
  return S;
  #return Bo;  
end;</Text-field>
