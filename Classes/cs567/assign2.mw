<Group labelreference="L834" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">imont := proc( X, Y, N, b )
  local q, t1, t2, t3, t4, A, m, k;
  local qdigits, qa;
  m := ArrayNumElems( X );  
  A := Array( 0 .. m-1 );
  #print( convert( A, list ) );
  for k from m-1 by -1 to 0 do
    q := (A[m-1] + X[k]*Y[m-1]) * (1/(b-N[m-1]) mod b) mod b;    
    #qdigits := convert( q, base, b );
    #qa := Array(0..nops(qdigits)-1,FlipDimension(Array(0..nops(qdigits)-1,qdigits),2));
    #print(convert( qa, list ));
    t1 := imult( X[k..k], Y, b );
    t2 := imult( Array(0 .. 0, [q]), N, b );
    t3 := iadd( A, t1, b, 'Carr1' );
    if Carr1 = 1 then
      t3 := Concatenate( 2, Array(0 .. 0, [1]), t3 );
      #print(Carr1);
    end if;
    t4 := iadd( t3, t2, b, 'Carr2' );
    if Carr2 = 1 then
      t4 := Concatenate( 2, Array(0 .. 0, [1]), t4 );
      #print(Carr2);
    end if;
    A := idiv( t4, Array(0 .. 1, [1,0]), b, 'r' );
  end do;
  return A;
end;</Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display=""></Equation></Text-field>
