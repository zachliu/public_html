<Group labelreference="L857" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">FFTmult := proc( A, B, radix )
  local a, b, c, dc, cc, temp, i;
  local m, n;
  local N, aa, bb, Aa, Bb, Cc, C;
  local t0, t1, T;
  a := inttopoly(A,radix,x);
  b := inttopoly(B,radix,x);
  m := degree(a,x);
  n := degree(b,x);
  #print(m);
  #print(n);
  N := 2^ceil(log[2](m+n+1));
  aa := Vector(N,0,datatype=complex[8]);
  bb := Vector(N,0,datatype=complex[8]);
  #c := expand(a*b);
  #sort(c,x,ascending);
  #print(subs(x=radix,c));  
  #return;

  t0 := time();
  for i from 0 to m do
    aa[i+1] := coeff(a,x,i);
  end do;
  for i from 0 to n do
    bb[i+1] := coeff(b,x,i);
  end do;
  t1 := time();
  <Font encoding="UTF-8">T := t1 - t0;
  printf(&quot;Time for extracting coefficients: %f sec\134n&quot;, T);
  </Font>
  t0 := time();
  Aa := FourierTransform(aa,normalization=none);
  Bb := FourierTransform(bb,normalization=none);
  t1 := time();
  T := t1 - t0;
  printf(&quot;Time for FFT transform:           %f sec<Font encoding="UTF-8">\134n</Font>&quot;, T);
  
  t0 := time();
  Cc := Vector(N,0,datatype=complex[8]);
  for i from 1 to N do
    Cc[i] := (1/N)*Aa[i]*Bb[i];
  end do;
  C := InverseFourierTransform(Cc,normalization=none);
  cc := add(round(Re(C[i]))*x^(i-1),i=1..N);
  t1 := time();
  T := t1 - t0;
  printf(&quot;Time for mult and iFFT:           %f sec<Font encoding="UTF-8">\134n</Font>&quot;, T);

  return subs(x=radix,cc);
end;</Text-field>
</Input>
