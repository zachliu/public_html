<Section collapsed="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title>
<Text-field style="Heading 2" layout="Heading 2">The 'add' routine - two integers addition</Text-field></Title>
<Group labelreference="L726" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">iadd := proc( A1, B1, b, Carr::name )
  local A, B, Ac, Bc, m, n, S, Ca, i;
  m := ArrayNumElems(A1);
  n := ArrayNumElems(B1);

  A := Array(0..m-1);
  B := Array(0..n-1);

  Copy(A1,A);
  Copy(B1,B);

  if m &gt; n then    
    Bc := Array(0..m-1);
    for i from 0 to m-1 do
      if i &lt;= m-n-1 then
        Bc[i] := 0;
      else
        Bc[i] := B[i-(m-n)]
      end if;
    end do;
    #print(convert(Bc, list));
    Ac := A;
  end if;
  if m &lt; n then
    Ac := Array(0..n-1);
    for i from 0 to n-1 do
      if i &lt;= n-m-1 then
        Ac[i] := 0;
      else
        Ac[i] := A[i-(n-m)]
      end if;
    end do;
    #print(convert(Ac, list));
    Bc := B;
  end if;
  if m = n then
    Ac := A;
    Bc := B;
  end if;

  S := Array(0..max(m,n)-1);
  Ca := 0;
  for i from max(m,n)-1 by -1 to 0 do
    if Ac[i] + Bc[i] + Ca &lt; b then
      S[i] := Ac[i] + Bc[i] + Ca;
      Ca := 0;
    else
      S[i] := Ac[i] + Bc[i] + Ca - b;
      Ca := 1;
    end if;
  end do;
  Carr := Ca;
  return S;
  #return Ca;  
end;</Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display=""></Equation></Text-field>
</Output>
