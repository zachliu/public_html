<Group view="code" labelreference="L388" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">fdiv := proc(A,B,m,n)
  local Q, R;
  local A1, mu, Q1, Qh1, R1;
  local cnt;
  #Q := iquo(A,B);
  #R := irem(A,B);
  #Q := floor(A/B);
  #R := A - B*Q;
  A1 := floor( A * 10^(-(n-1)) );
  mu := floor( 10^m/B );
  Q1 := floor( A1 * mu * 10^(-(m-n+1)) );
  Qh1 := floor( A * mu / 10^m );
  R1 := A - B * Q1;
  cnt := 0;
  while R1 &gt; B do
    Q1  := Q1 + 1;
    cnt := cnt + 1;
    R1  := R1 - B;
  end do;
  #print('`# of times of increments`'=cnt);
  #print(Q1);
  #print(R1);
  return cnt;
end;</Text-field>
</Input>
<Output>
<Text-field style="2D Output" layout="Maple Output"><Equation executable="false" style="2D Output" display=""></Equation></Text-field>
